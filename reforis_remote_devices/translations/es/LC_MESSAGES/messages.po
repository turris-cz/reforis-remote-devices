#: js/src/app.js:11
msgid "Remote Devices"
msgstr "Dispositivos Remotos"

#: js/src/remoteDevices/AddDeviceForm.js:56
msgid "Add Device"
msgstr "Añadir un dispositivo"

#: js/src/remoteDevices/AddDeviceForm.js:58
msgid "Upload token file generated by the device you want to control remotely."
msgstr ""
"Subir el archivo token generado por el dispositivo que quieres controlar "
"de forma remota."

#: js/src/remoteDevices/AddDeviceForm.js:67
msgid "Choose token file..."
msgstr "Elegir archivo token..."

#: js/src/remoteDevices/AddDeviceForm.js:86
msgid "Upload token"
msgstr "Subir token"

#: js/src/remoteDevices/AddDeviceForm.js:101
msgid "The file is too big. The maximum size is 1 MB."
msgstr "El archivo es demasiado grande. El tamaño máximo es de 1 MB."

#: js/src/remoteDevices/AddDeviceForm.js:107
msgid "The filename must be at least 1 and at most 50 characters long."
msgstr "El nombre de fichero debe tener una longitud entre 1 y 50 caracteres."

#: js/src/remoteDevices/AddDeviceForm.js:114
msgid ""
"The filename can contain only alphanumeric characters, dots, dashes, and "
"underscores."
msgstr ""
"El archivo sólo puede contener caracteres alfanuméricos, puntos, guiones "
"y guiones bajos."

#: js/src/remoteDevices/Devices.js:50
msgid "Devices List"
msgstr "Lista de dispositivos"

#: js/src/remoteDevices/DevicesTable.js:42
msgid "No devices added yet."
msgstr "No se han añadido dispositivos actualmente."

#: js/src/remoteDevices/DevicesTable.js:52
msgid "ID"
msgstr "ID"

#: js/src/remoteDevices/DevicesTable.js:53
msgid "Name"
msgstr "Nombre"

#: js/src/remoteDevices/DevicesTable.js:55
msgid "Status"
msgstr "Estado"

#: js/src/remoteDevices/DevicesTable.js:58
msgid "Managed"
msgstr "Gestionado"

#: js/src/remoteDevices/DevicesTable.js:63
#: js/src/remoteDevices/DevicesTable.js:146
msgid "Delete"
msgstr "Eliminar"

#: js/src/remoteDevices/DevicesTable.js:133
msgid "Yes"
msgstr "Sí"

#: js/src/remoteDevices/DevicesTable.js:133
msgid "No"
msgstr "No"

#: js/src/remoteDevices/DevicesTable.js:162
msgid "Started"
msgstr "Iniciado"

#: js/src/remoteDevices/DevicesTable.js:167
msgid "Running"
msgstr "En ejecución"

#: js/src/remoteDevices/DevicesTable.js:172
msgid "Exited"
msgstr "Terminado"

#: js/src/remoteDevices/DevicesTable.js:177
msgid "Unknown status"
msgstr "Estado desconocido"

#: js/src/remoteDevices/RemoteDevices.js:22
msgid "Devices"
msgstr "Dispositivos"

#: js/src/remoteDevices/RemoteDevices.js:24
msgid "You can set up other Turris routers to be managed remotely by this device."
msgstr ""
"Puedes configurar otros routers Turris para ser gestionados remotamente "
"por este dispositivo."

#: js/src/remoteDevices/RemoteDevices.js:29
msgid ""
"To authorize your router, you will need a token file that can be obtained"
" with the \"Remote Access\" plugin on the target device."
msgstr ""
"Para autorizar tu router, necesitarás un token que se puede obtener con "
"el complemento \"Acceso remoto\" en el dispositivo deseado."

#: js/src/remoteDevices/editableName/EditableName.js:71
msgid "Custom name"
msgstr "Nombre personalizado"

#: js/src/remoteDevices/editableName/EditableName.js:92
msgid "Edit name"
msgstr "Renombrar"

#: js/src/remoteDevices/editableName/SubmitOrDiscard.js:32
msgid "Save changes"
msgstr "Guardar cambios"

#: js/src/remoteDevices/editableName/SubmitOrDiscard.js:43
msgid "Discard changes"
msgstr "Descartar cambios"

#: reforis_remote_devices/__init__.py:42
msgid "Missing data for 'token' file."
msgstr "Falta información del archivo «token»."

#: reforis_remote_devices/__init__.py:52
msgid "Cannot add remote device token. Check if device is already added."
msgstr ""
"No se puede añadir el token del dispositivo remoto. Verifica si el "
"dispositivo ya se encuentra añadido."

#: reforis_remote_devices/__init__.py:64
msgid "Cannot delete device."
msgstr "No se puede eliminar el dispositivo."

#: reforis_remote_devices/__init__.py:79
msgid "Missing data for \"options\" or \"enabled\" field."
msgstr "Falta información para los campos «options» o «enabled»."

#: reforis_remote_devices/__init__.py:85
msgid "Name is too long. Maximum 30 characters are allowed."
msgstr "El nombre es demasiado largo. Se permiten hasta 30 caracteres."

#: reforis_remote_devices/__init__.py:104
msgid "Cannot update device."
msgstr "No se puede actualizar el dispositivo."

